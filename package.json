    "@babel/preset-typescript": "^7.15.0",
    "@chakra-ui/icons": "^1.0.14",
    "@chakra-ui/react": "^1.6.5",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@fontsource/roboto": "^4.5.0",
    "@svgr/webpack": "^5.5.0",
    "@types/enzyme": "^3.10.9",
    "@types/jest": "^26.0.24",
    "@types/next-auth": "^3.15.0",
    "@types/node": "^16.4.6",
    "@types/react": "17.0.15",
    "@types/shelljs": "^0.8.9",
    "@types/sinon": "^10.0.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-cli": "^6.26.0",
    "babel-jest": "^27.0.6",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "eslint": "^7.31.0",
    "eslint-config-next": "11.0.1",
    "eslint-plugin-import": "^2.24.0",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "jest": "^27.0.6",
    "lerna": "^4.0.0",
    "lodash": "^4.17.21",
    "next": "^11.1.0",
    "next-auth": "^3.27.3",
    "next-seo": "^4.26.0",
    "node-plop": "^0.26.2",
    "nodemon": "^2.0.12",
    "plop": "^2.7.4",
    "react": "17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "sinon": "^11.1.2",
    "ts-node": "^10.1.0",
    "typedoc": "^0.21.5",
    "typescript": "4.3.5"
  }
