{
  "name": "hypernext",
  "private": true,
  "scripts": {
    "prestart": "npm install && npm run bootstrap",
    "build": "lerna run build --no-private --stream",
    "boot": "npm run prestart && npm run build",
    "lint": "lerna run lint --no-private --stream",
    "lint:types": "lerna run lint:types --no-private --stream",
    "bootstrap": "lerna bootstrap",
    "test": "jest --config jest.config.js --maxWorkers=50%",
    "test:cov": "jest --config jest.config.js --maxWorkers=50% --coverage",
    "clean": "lerna clean --yes && rimraf node_modules",
    "reset": "npm run clean && npm install && npm run bootstrap",
    "clean-dist": "lerna exec -- rimraf dist",
    "create:pkg": "ts-node scripts/create-pkg.ts"
  },
  "dependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
